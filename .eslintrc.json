{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
    },
   "plugins": [
      "svelte3"
   ],
   "overrides": [
      {
         "files": ["*.svelte"],
         "processor": "svelte3/svelte3"
      }
   ]
}
